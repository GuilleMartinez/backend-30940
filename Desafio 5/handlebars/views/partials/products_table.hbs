<table>

  <caption>
    Products View
  </caption>

  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Price</th>
      <th scope="col">Thumbnail</th>
    </tr>
  </thead>

  <tbody>
    {{#each products }}
    <tr>
      <td>{{this.title}}</td>
      <td>{{this.price}}$</td>
      <td>
        <img alt="" style="object-fit: cover; height: 90px; width: 90px" src="{{this.thumbnail}}" />
      </td>
    </tr>
    {{/each }}
  </tbody>

</table>